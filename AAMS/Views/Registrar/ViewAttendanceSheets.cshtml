@model IEnumerable<AAMS.Models.AttendanceSheet>

@{
    ViewBag.Title = "ViewAttendanceSheets";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ViewAttendanceSheets</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StdId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CourseId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Section)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AssignedLecturerId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StdId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CourseId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Section)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AssignedLecturerId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AttendanceSheetId }) |
            @Html.ActionLink("Details", "Details", new { id=item.AttendanceSheetId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.AttendanceSheetId })
        </td>
    </tr>
}

</table>
